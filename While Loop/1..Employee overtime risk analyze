#include<stdio.h>
int main() {
    int n;
    scanf("%d",&n);
    int i=0;
    int hours;
    int totalOvertime=0;
    int burnoutDays=0;
    while(i<n) {
        scanf("%d",&hours);
        totalOvertime+=hours;
        if (hours>4) {
            burnoutDays++;
        }
        i++;
    }
    printf("Total Overtime : %d\n",totalOvertime);
    printf("Burnout Days: %d\n",burnoutDays);
    return 0;
}
